{
    "manifest_version": 3,
    "name": "Phishing Alert Extension",
    "version": "1.0",
    "description": "의심스러운 사이트 접속 시 경고 알림 제공",
    "permissions": [
      "tabs",
      "scripting"
    ],
    "host_permissions": [
      "<all_urls>"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_title": "Phishing Alert"
    }
  }
  